Begin MrBayes;
  execute data.nex;

  charset binary=1-100;
  charset multi =101-200;
  partition two =2:binary,multi;
  set partition =two;

  lset applyto=(all) coding=variable;

  prset applyto=(1) symdirihyperpr=exp(1);

  prset nodeagepr=calibrated;
  prset brlenspr=clock:fossilization;
  prset speciationpr=exp(1);
  prset extinctionpr=beta(1,1);
  prset fossilizationpr=beta(1,1);
  prset sampleprob=1.0;
  prset treeagepr=gamma(1,1);

  prset clockratepr=exp(1);
  prset clockvarpr =iln;

  mcmcp nchain=1 ngen=8000000 samplefr=250 printfr=100000 diagnfr=500000;

  [propset dirichlet(pi_symdir)$prob=15;]

  mcmc;
  sumt burninfrac=0.4 contype=all;
  sump burninfrac=0.4;
End;
